	ProxyCommand ~/bin/homeproxy %h %p
	Compression yes

Host naoko.eatnumber1.com
	User eatnumber1

Host *.eatnumber1.com *.csh.rit.edu
	User russ
	ForwardAgent yes

Host 192.168.1.*
	StrictHostKeyChecking no
	UserKnownHostsFile /dev/null

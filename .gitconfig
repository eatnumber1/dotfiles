[user]
	name = Russell Harmon
	email = eatnumber1@gmail.com
[core]
	ignorecase = false
	excludesfile = ~/.gitignore.global
[color]
	diff = auto
	branch = auto
	grep = auto
	interactive = auto
	showbranch = auto
	status = auto
[github]
	user = eatnumber1
[include]
	path = .gitconfig.local
	path = .gitconfig.google
[push]
	default = upstream
[alias]
	assume = update-index --assume-unchanged
	unassume = update-index --no-assume-unchanged
	assumed  = "!git ls-files -v | grep ^h | cut -c 3-"
[diff]
	algorithm = patience
[credential]
	helper = osxkeychain
[pull]
	rebase = true
[sendemail]
	smtpEncryption = tls
	smtpServer = smtp.gmail.com
	smtpUser = eatnumber1@gmail.com
	smtpServerPort = 587

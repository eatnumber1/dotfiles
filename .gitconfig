[core]
	ignorecase = false
	excludesfile = ~/.gitignore.global
[color]
	diff = auto
	branch = auto
	grep = auto
	interactive = auto
	showbranch = auto
	status = auto
[github]
	user = eatnumber1
[include]
	path = .gitconfig.local
	path = .gitconfig.google
[push]
	default = upstream
[alias]
	assume = update-index --assume-unchanged
	unassume = update-index --no-assume-unchanged
	assumed  = "!git ls-files -v | grep ^h | cut -c 3-"
[diff]
	algorithm = patience
[sendemail]
	from = eatnumber1@google.com
[credential]
	helper = osxkeychain
[pull]
	rebase = true
[gpg]
	format = ssh
[gpg "ssh"]
	program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"
[commit]
	gpgsign = true
